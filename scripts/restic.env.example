# Copy to scripts/restic.env and fill in. Keep this file private.
# chmod 600 scripts/restic.env

# Examples:
# RESTIC_REPOSITORY=sftp:user@server:/backups/openclaw-workspace
# RESTIC_REPOSITORY=b2:my-bucket:openclaw-workspace
RESTIC_REPOSITORY=

# Strong passphrase (do not reuse)
RESTIC_PASSWORD=

# Optional: S3/B2 credentials, only if your backend needs them
# AWS_ACCESS_KEY_ID=
# AWS_SECRET_ACCESS_KEY=
# B2_ACCOUNT_ID=
# B2_ACCOUNT_KEY=
